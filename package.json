{
    "name": "dims-uiot",
    "version": "1.0.0",
    "author": "uIoT",
    "description": "Modulo DIMS do Middleware uIoT",
    "scripts": {
        "dev": "nodemon -L --exec ts-node src/server.ts --watch src",
        "dev:no-watch": "ts-node src/server.ts",
        "build": "rimraf dist && tsc",
        "start": "node dist/server.js",
        "db:push": "drizzle-kit push",
        "db:generate": "drizzle-kit generate",
        "db:generate:custom": "drizzle-kit generate --custom",
        "db:migrate": "drizzle-kit migrate",
        "db:deleteMigrations": "drizzle-kit drop",
        "db:studio": "drizzle-kit studio --host 127.0.0.1",
        "db:seed": "ts-node src/db/seed.ts"
    },
    "dependencies": {
        "@fastify/cors": "^11.0.1",
        "@fastify/helmet": "^13.0.1",
        "@fastify/swagger": "^9.4.2",
        "@fastify/swagger-ui": "^5.2.2",
        "@influxdata/influxdb-client": "^1.35.0",
        "@influxdata/influxdb-client-apis": "^1.35.0",
        "date-fns": "^4.1.0",
        "dotenv-safe": "^9.1.0",
        "drizzle-orm": "^0.41.0",
        "drizzle-zod": "^0.7.1",
        "envalid": "^8.0.0",
        "fastify": "^5.2.2",
        "fastify-type-provider-zod": "^4.0.2",
        "http-errors": "^2.0.0",
        "postgres": "^3.4.5",
        "rimraf": "^6.0.1",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/dotenv-safe": "^8.1.6",
        "@types/http-errors": "^2.0.4",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.14.0",
        "@types/pg": "^8.11.11",
        "@typescript-eslint/eslint-plugin": "^8.29.1",
        "@typescript-eslint/parser": "^8.29.1",
        "drizzle-kit": "^0.30.6",
        "eslint": "^9.24.0",
        "eslint-config-love": "^119.0.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-n": "^17.17.0",
        "eslint-plugin-promise": "^7.2.1",
        "nodemon": "^3.1.9",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}